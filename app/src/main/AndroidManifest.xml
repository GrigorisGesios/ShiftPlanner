<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.shiftplanner">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ShiftPlanner">
        <activity android:name=".Employee.DaysOff" />
        <activity android:name=".Employee.ViewFinalSchedule" />
        <activity android:name=".Employee.GiveRequirements" />
        <activity android:name=".Employee.EmployeeLayout" />
        <activity android:name=".Manager.PlanFinalSchedule" />
        <activity android:name=".Manager.ReviewRequirements" />
        <activity android:name=".Manager.GiveRestrictions" />
        <activity android:name=".Manager.ManagerLayout" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>